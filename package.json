{
  "name": "character-speed-calculator",
  "version": "1.0.0",
  "description": "Gaming character speed calculator using TypeScript and Express.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:lambda": "tsc && cp package.json dist/ && cd dist && npm install --production",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "watch": "tsc -w",
    "cdk:deploy": "npm run build:lambda && cdk deploy",
    "cdk:destroy": "cdk destroy",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth"
  },
  "keywords": [
    "gaming",
    "character",
    "speed",
    "calculator",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.883.0",
    "@aws-sdk/client-secrets-manager": "^3.490.0",
    "@aws-sdk/client-ssm": "^3.490.0",
    "aws-jwt-verify": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.3.0",
    "serverless-http": "^3.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/http-status-codes": "^1.2.0",
    "@types/node": "^18.19.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "aws-cdk": "^2.120.0",
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.3.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
